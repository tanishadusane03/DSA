public class Solution {
    
    public static void bubbleSort(int[] a, int n) {   
        
        for(int i=n-1;i>=0;i--){ //0 to n-1 then 0 to n-2 .....
            for(int j=0;j<=i-1;j++){ //j<n-2 i=n-1-1=n-2
                if(a[j]>a[j+1]){
                    int temp=a[j];
                    a[j]=a[j+1];
                    a[j+1]=temp;
                }
            }
        }

    }

}
