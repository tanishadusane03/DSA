class HelloWorld {
    public static void main(String[] args) {
        
       int arr[]= {1,2,3,4,5};
       int n =arr.length;
       int a=0;
       int b=n-1;
      
       
       while(a<b){
           int swap=arr[a];
          arr[a]=arr[b];
           arr[b]=swap;
           a++;
           b--;
           
       }
       for(int k=0;k<n;k++){
           System.out.println(arr[k]);
       }
       
    }
   
}
